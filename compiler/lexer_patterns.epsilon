
# Run with:
# python -m epsilon.cli ../crown-os/compiler/lexer_patterns.ini

[example]

id = [a-zA-Z][A-Za-z_0-9]*
space = [ ]+
number = [0-9]+
comment = \#[^\n]*\n
newline = \n
minus = \-
plus = \+
equals = \=
less = \<
greater = \>
less_equals = \<\=
greater_equals = \>\=
colon = \:
coloncolon = \:\:
dot = \.
comma = \,
arrow = \-\>
brace_open = \(
brace_close = \)
string = \"[^\"\n]*\"
